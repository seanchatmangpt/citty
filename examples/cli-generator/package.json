{
  "name": "cli-generator-unjucks",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "CLI tool generator with UnJS integrations using UnJucks",
  "bin": {
    "cli-gen": "./dist/cli.mjs"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
