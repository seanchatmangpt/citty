    "pathe": "^1.1.2",
    "prompts": "^2.4.2",
    "scule": "^1.3.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  }
