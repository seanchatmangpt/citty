      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./pqc": {
      "types": "./pqc/index.d.ts",
      "import": "./pqc/index.js"
    },
    "./ai-engine": {
      "types": "./ai-engine/index.d.ts",
      "import": "./ai-engine/index.js"
    },
    "./security": {
      "types": "./security/index.d.ts",
      "import": "./security/index.js"
    },
    "./performance": {
      "types": "./performance/index.d.ts",
      "import": "./performance/index.js"
    }
  },
  "scripts": {
