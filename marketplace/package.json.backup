{
  "name": "citty-pro-marketplace",
  "version": "1.0.0",
  "description": "Citty Pro Ecosystem Marketplace - Discover and share CLI tools, templates, and workflows",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:e2e": "vitest run tests/e2e/",
    "test:performance": "VITEST_PERFORMANCE=true vitest run tests/performance/",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=json --reporter=verbose",
    "benchmark": "vitest bench",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "typecheck": "vue-tsc --noEmit",
    "api": "tsx server/api/index.ts",
    "clean": "rm -rf .nuxt .output dist coverage test-results.json test-report.html"
  },
  "dependencies": {
    "@nuxt/ui": "^2.18.7",
    "@pinia/nuxt": "^0.5.5",
    "@types/bcrypt": "^6.0.0",
    "@vueuse/nuxt": "^11.1.0",
    "bcrypt": "^6.0.0",
    "citty": "^0.1.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crossws": "^0.4.1",
    "crypto-js": "^4.2.0",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.1",
    "node-cache": "^5.1.2",
    "pinia": "^2.2.6",
    "rate-limiter-flexible": "^7.3.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/eslint-config": "^0.7.3",
    "@nuxtjs/color-mode": "^3.5.1",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fast-check": "^3.23.2",
    "jsdom": "^26.1.0",
    "nuxt": "^3.19.0",
    "prettier": "^3.4.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.11"
  },
  "keywords": [
    "citty",
    "cli",
    "marketplace",
    "templates",
