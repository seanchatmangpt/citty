{
  "name": "@unjs/unjucks",
  "version": "0.1.0",
  "description": "ðŸ§¬ Universal template system with AI-powered ontology-driven context management for the UnJS ecosystem. Build semantic, type-safe templates with cognitive context understanding and multi-framework compatibility.",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./context": {
      "import": "./dist/context.mjs",
      "require": "./dist/context.cjs",
      "types": "./dist/context.d.ts"
    },
    "./walker": {
      "import": "./dist/walker.mjs",
      "require": "./dist/walker.cjs",
      "types": "./dist/walker.d.ts"
    },
    "./renderer": {
      "import": "./dist/renderer.mjs",
      "require": "./dist/renderer.cjs",
      "types": "./dist/renderer.d.ts"
    },
    "./ontology": {
      "import": "./dist/ontology.mjs",
      "require": "./dist/ontology.cjs",
      "types": "./dist/ontology.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs",
      "types": "./dist/cli.d.ts"
    },
    "./cli-enhanced": {
      "import": "./dist/cli-enhanced.mjs",
      "require": "./dist/cli-enhanced.cjs",
      "types": "./dist/cli-enhanced.d.ts"
    },
    "./pro": {
      "import": "./dist/pro/index.mjs",
      "require": "./dist/pro/index.cjs",
      "types": "./dist/pro/index.d.ts"
    },
    "./ai": {
      "import": "./dist/ai/index.mjs",
      "require": "./dist/ai/index.cjs",
      "types": "./dist/ai/index.d.ts"
    },
    "./weaver": {
      "import": "./dist/weaver/index.mjs",
      "require": "./dist/weaver/index.cjs",
      "types": "./dist/weaver/index.d.ts"
    },
    "./commands/*": {
      "import": "./dist/commands/*.mjs",
      "require": "./dist/commands/*.cjs",
      "types": "./dist/commands/*.d.ts"
    },
    "./nuxt": {
      "import": "./dist/integration/nuxt-module.mjs",
      "require": "./dist/integration/nuxt-module.cjs",
      "types": "./dist/integration/nuxt-module.d.ts"
    },
    "./nitro": {
      "import": "./dist/integration/nitro-plugin.mjs",
      "require": "./dist/integration/nitro-plugin.cjs",
      "types": "./dist/integration/nitro-plugin.d.ts"
    },
    "./vite": {
      "import": "./dist/integration/vite-plugin.mjs",
      "require": "./dist/integration/vite-plugin.cjs",
      "types": "./dist/integration/vite-plugin.d.ts"
    },
    "./h3": {
      "import": "./dist/integration/h3-middleware.mjs",
      "require": "./dist/integration/h3-middleware.cjs",
      "types": "./dist/integration/h3-middleware.d.ts"
    },
    "./templates/*": "./templates/*",
    "./package.json": "./package.json"
  },
  "bin": {
    "unjucks": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:types": "vitest --typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docs:build": "typedoc --out docs src/index.ts",
    "docs:dev": "typedoc --watch --out docs src/index.ts",
    "playground": "tsx src/playground/interactive-playground.ts",
    "example:nuxt": "cd examples/nuxt && pnpm dev",
    "example:nitro": "cd examples/nitro && pnpm dev",
    "example:vite": "cd examples/vite && pnpm dev",
    "benchmark": "tsx src/benchmarks/performance.ts",
    "validate:ecosystem": "pnpm run build && pnpm run test:ci && pnpm run typecheck && pnpm run lint",
    "changelog": "changelogen",
    "prepublishOnly": "pnpm run validate:ecosystem",
    "release": "changelogen --release && pnpm publish && git push --follow-tags",
    "release:beta": "changelogen --prerelease beta && pnpm publish --tag beta",
    "postinstall": "node -e \"console.log('ðŸ§¬ unjucks: Universal template system ready! Try: npx unjucks --help');\""
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "chalk": "^5.3.0",
    "citty": "^0.1.6",
    "cli-table3": "^0.6.3",
    "colorette": "^2.0.20",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "jest-diff": "^29.7.0",
    "n3": "^1.17.3",
    "nunjucks": "^3.2.4",
    "ofetch": "^1.3.3",
    "ohash": "^1.1.6",
    "ora": "^8.0.1",
    "p-queue": "^8.0.1",
    "pathe": "^1.1.2",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "scule": "^1.3.0",
    "unctx": "^2.3.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/node": "^20.10.6",
    "@types/nunjucks": "^3.2.6",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "ai": "^5.0.33",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "fast-check": "^4.3.0",
    "hookable": "^5.5.3",
    "ollama-ai-provider-v2": "^1.2.4",
    "typescript": "^5.9.2",
    "unbuild": "^2.0.0",
    "undici": "^7.15.0",
    "vitest": "^1.1.3",
    "vm2": "^3.9.19",
    "zod": "^3.25.76"
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.0.0",
    "pnpm": ">=7.0.0"
  },
