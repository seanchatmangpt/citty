{
  "name": "citty-pro",
  "version": "1.0.0",
  "description": "AI-Powered Autonomous CLI Generator with Ollama, Weaver Forge, and OpenTelemetry",
  "repository": "unjs/citty",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --cache . && prettier -c src test",
    "lint:fix": "eslint --cache . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "demo": "jiti ./src/commands/generate-pro.ts --help",
    "release": "pnpm test && changelogen --release --push && npm publish",
    "test": "pnpm lint && vitest run --coverage",
    "generate:ai": "jiti ./src/commands/generate-pro.ts",
    "otel:setup": "node ./dist/otel/instrumentation.js"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.46.1",
    "@opentelemetry/exporter-prometheus": "^0.52.1",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.25.1",
    "@types/nunjucks": "^3.2.6",
    "@types/yaml": "^1.9.7",
    "ai": "^5.0.31",
    "chalk": "^5.3.0",
    "consola": "^3.4.0",
    "fast-glob": "^3.3.2",
    "inquirer": "^10.2.2",
    "js-yaml": "^4.1.0",
    "n3": "^1.26.0",
    "nunjucks": "^3.2.4",
    "ollama-ai-provider-v2": "^1.2.4",
    "ora": "^8.1.0",
    "semver": "^7.6.3",
    "uuid": "^10.0.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.8",
    "automd": "^0.4.0",
    "changelogen": "^0.6.1",
    "eslint": "^9.22.0",
    "eslint-config-unjs": "^0.4.2",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "scule": "^1.3.0",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.8"
  },
  "packageManager": "pnpm@10.6.1"
}