{
  "name": "@unjs/unjucks",
  "version": "0.1.0",
  "description": "Universal template system with ontology-driven context management",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./context": {
      "import": "./dist/context.js",
      "types": "./dist/context.d.ts"
    },
    "./walker": {
      "import": "./dist/walker.js",
      "types": "./dist/walker.d.ts"
    },
    "./renderer": {
      "import": "./dist/renderer.js",
      "types": "./dist/renderer.d.ts"
    },
    "./ontology": {
      "import": "./dist/ontology.js",
      "types": "./dist/ontology.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "bin": {
    "unjucks": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "release": "changelogen --release && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "chalk": "^5.3.0",
    "citty": "^0.1.6",
    "cli-table3": "^0.6.3",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "jest-diff": "^29.7.0",
    "n3": "^1.17.3",
    "nunjucks": "^3.2.4",
    "ofetch": "^1.3.3",
    "ohash": "^1.1.3",
    "ora": "^8.0.1",
    "p-queue": "^8.0.1",
    "pathe": "^1.1.2",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "scule": "^1.3.0",
    "unctx": "^2.3.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/node": "^20.10.6",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "fast-check": "^4.3.0",
    "hookable": "^5.5.3",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "template",
    "nunjucks",
    "generator",
    "ontology",
    "context",
    "cli",
    "unjs"
  ],
  "author": "UnJS Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unjs/unjucks.git"
  },
  "bugs": {
    "url": "https://github.com/unjs/unjucks/issues"
  },
  "homepage": "https://github.com/unjs/unjucks#readme"
}